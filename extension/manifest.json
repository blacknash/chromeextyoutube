{
  "manifest_version": 2,
  "name": "Youtube Video Extension",
  "description": "This extension simplifies the youtube video's search",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts":["background.js"]
  },
  "permissions": [
    "identity",
    "activeTab",
    "tabs"
  ],
  "oauth2": {
    "client_id" : "191727044633-trjdt3pstjfnq2m7qpsuducgdpqmnnf7.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/youtube"
    ]
  }
}